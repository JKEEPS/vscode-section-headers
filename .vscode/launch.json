{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/vscode-section-headers"
      ],
      "outFiles": [
        "${workspaceFolder}/vscode-section-headers/out/**/*.js"
      ],
      "preLaunchTask": "ext: build"
    }
  ]
}
